<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lee989898.soptlee">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".SOPTApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SOPTLEE"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.lee989898.soptlee.ui.onboarding.OnBoardingActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.lee989898.soptlee.ui.setting.SettingActivity"
            android:exported="false" />
        <activity
            android:name="com.lee989898.soptlee.ui.detail.DetailActivity"
            android:exported="false" />
        <activity
            android:name="com.lee989898.soptlee.ui.signup.SignUpActivity"
            android:exported="false" />
        <activity
            android:name="com.lee989898.soptlee.ui.main.HomeActivity"
            android:exported="false" />
        <activity
            android:name="com.lee989898.soptlee.ui.signin.SignInActivity"
            android:exported="true">

        </activity>
    </application>

</manifest>